{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -k \"npm:dev:api\" \"next dev\"",
    "dev:api": "chokidar ../src/main/resources/openapi/causa-api.yaml -c \"npm run gen:api\"",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest run",
    "gen:api": "openapi-typescript ../src/main/resources/openapi/causa-api.yaml -o src/lib/api/types.ts"
  },
  "dependencies": {
    "next": "16.1.1",
    "openapi-fetch": "^0.15.0",
    "react": "19.2.3",
    "react-day-picker": "^9.13.0",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "jsdom": "^27.4.0",
    "openapi-typescript": "^7.6.1",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^1.6.0"
  }
}
